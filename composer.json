{
  "name": "rmts/laravel-akamai",
  "description": "Simple AKAMAI API integration for Laravel",
  "keywords": [
    "framework",
    "laravel",
    "api"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=7.2",
    "ext-json": "*",
    "laravel/framework": "^7.0|^8.0",
    "patriziotomato/edgegrid-client": "^1.1"
  },
  "require-dev": {
    "fzaninotto/faker": "^1.9.1",
    "nunomaduro/collision": "^4.2",
    "phpunit/phpunit": "^8.0",
    "orchestra/testbench": "^5.2"
  },
  "repositories": [
    {
      "type": "git",
      "url": "git@github.com:patriziotomato/AkamaiOPEN-edgegrid-php-client.git"
    }
  ],
  "autoload": {
    "psr-4": {
      "LaravelAkamai\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LaravelAkamaiTest\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": {
      "rmts/*": "source",
      "*": "dist"
    },
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "LaravelAkamai\\LaravelAkamaiServiceProvider"
      ],
      "aliases": {
      }
    }
  }
}
